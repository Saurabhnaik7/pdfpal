generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Document {
  id        String   @id @default(cuid())
  userId    String
  fileUrl   String
  fileName  String
  createdAt DateTime @default(now()) @map("created_at")

  @@map("documents")
}
